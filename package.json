{
  "name": "vue-application",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.ts,.tsx --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.16",
    "axios": "^1.8.4",
    "pinia": "^3.0.1",
    "tailwindcss": "^4.0.16",
    "vue": "^3.4.21",
    "vue-i18n": "^9.14.3",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.13",
    "@types/vue-router": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "typescript": "~5.3.3",
    "vite": "^5.1.4",
    "vue-tsc": "^1.8.27"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
